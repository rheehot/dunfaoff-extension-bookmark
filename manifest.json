{
  "manifest_version": 2,
  "name": "dunfaoff-bookmark",
  "description": "extension for dunfaoff bookmark",
  "version": "0.0.1",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "src/packages/background/index.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://dunfaoff.com/SearchResult.df*"
      ],
      "all_frames": true,
      "js": [
        "./src/packages/content/js/toastify.min.js",
        "./src/packages/content/js/recent-visit.js",
        "./src/packages/content/index.js"
      ],
      "css": [
        "./src/packages/content/style/toastify.min.css",
        "./src/packages/content/style/index.css"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "src/packages/popup/views/popup.html",
    "default_title": "Default Title"
  }
}